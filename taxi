    if (typeof ApiRoutes === 'undefined') {
        var ApiRoutes = {};
    }

    ApiRoutes['getUser'] = "https://hurryupcabs.com/user";
    ApiRoutes['registerUser'] = "https://hurryupcabs.com/register-user";
    ApiRoutes['sendOTP'] = "https://hurryupcabs.com/return-booking/sendOTP";
    ApiRoutes['verifyOTP'] = "https://hurryupcabs.com/return-booking/verifyOTP";

